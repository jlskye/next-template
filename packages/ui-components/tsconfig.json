{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "tsBuildInfoFile": ".tsbuildinfo",
    "incremental": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "module": "ESNext",
    "moduleResolution": "node",
    "types": ["vitest/globals"],
    "skipLibCheck": true,
    "target": "ESNext",
    "strict": true,
    "noEmit": true
  },
  "include": ["src"]
}
